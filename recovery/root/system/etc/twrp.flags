/super            emmc        /dev/block/platform/bootdevice/by-name/super                        flags=display="Super";backup=1;flashimg=1             
/system	    auto	/dev/block/dm-3    flags=backup=1;display="System";flashimg=1;wipeingui
/vendor	    auto	/dev/block/dm-2    flags=backup=1;display="Vendor";flashimg=1;wipeingui
/product	    auto	/dev/block/dm-1   flags=backup=1;display="Product";flashimg=1;wipeingui
