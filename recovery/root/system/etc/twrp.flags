/dtbo               emmc      /dev/block/platform/bootdevice/by-name/dtbo                           flags=display="DTBO";flashimg=1;slotselect;
/otp                emmc      /dev/block/platform/bootdevice/by-name/otp                   
/tkv                emmc      /dev/block/platform/bootdevice/by-name/tkv                       
